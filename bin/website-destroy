aws s3 rm s3://logs.warnerbell.com --recursive

aws cloudformation delete-stack \
 --stack-name "CRC-s3buckets"

 aws cloudformation delete-stack \
 --stack-name "Cldfrnt-Distro"

 aws cloudformation delete-stack \
 --stack-name "CRC-ddbLambdaApi"